}], "Text",ExpressionUUID->"9766914b-c5aa-43c8-8b40-185a3960ff9b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["c", "d"], "=", 
   RowBox[{
    SuperscriptBox["m", 
     RowBox[{"e", " ", "d"}]], "=", 
    RowBox[{
     SuperscriptBox["m", 
      RowBox[{"1", "+", 

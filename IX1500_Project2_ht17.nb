Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MillerRabin", "[", "k_", 
    StyleBox["]", "Input"]}], "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"d", "=", 
       RowBox[{"n", "-", "1"}]}], ",", 
      RowBox[{"r", "=", "0"}], ",", "a", ",", "x", ",", "i", ",", 
      RowBox[{"result", "=", "True"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{"d", ",", "2"}], "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"d", "/=", "2"}], ";", 
        RowBox[{"r", "++"}]}]}], "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"2", ",", 
            RowBox[{"n", "-", "2"}]}], "}"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"x", "=", 
         RowBox[{"PowerMod", "[", 
          RowBox[{"a", ",", "d", ",", "n"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"x", "\[NotEqual]", "1"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"i", "=", "0"}], ",", 
             RowBox[{"i", "<", "r"}], ",", 
             RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"x", "\[Equal]", 
                 RowBox[{"n", "-", "1"}]}], ",", 
                RowBox[{"Continue", "[", "]"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"x", "=", 
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"x", " ", "x"}], ",", "n"}], "]"}]}]}]}], 
            "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"x", "\[NotEqual]", 
              RowBox[{"n", "-", "1"}]}], ",", 
             RowBox[{"result", "=", "False"}]}], "]"}]}]}], 
         "\[IndentingNewLine]", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", "k", "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "result"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",ExpressionUUID->"a47dfcc8-ce68-47fa-8c97-d00209f83dc7"],


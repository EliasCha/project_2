Cell["\<\
A very famous (and simple) encryption scheme is the Caesar cipher which is \
built on a cyclic alphabet shift (3 steps). Here\[CloseCurlyQuote]s an \
example using English block letters.\
\>", "Text",ExpressionUUID->"fb0396e1-ced3-441d-b167-ff5b7f99f3d3"],

Cell[BoxData[
 RowBox[{"ClearAll", "[", "\"\<`*\>\"", "]"}]], "Input",ExpressionUUID->"0f9f4dfb-7c00-4ff3-82f6-5ef4328d8ba0"],

Cell[BoxData[
 RowBox[{
  RowBox[{"shift", "=", "3"}], ";"}]], "Input",ExpressionUUID->"b9790e88-31e6-47a7-b8e2-ea02af221299"],

Cell[BoxData[
 RowBox[{"alphabet", "=", 
  RowBox[{"CharacterRange", "[", 
   RowBox[{"\"\<A\>\"", ",", "\"\<Z\>\""}], "]"}]}]], "Input",ExpressionUUID->\
"860f3e5b-0a5f-40c3-9e92-62c696c5deff"],

Cell[BoxData[
 RowBox[{"substitution", "=", 
  RowBox[{"RotateRight", "[", 
   RowBox[{"alphabet", ",", "shift"}], "]"}]}]], "Input",ExpressionUUID->\
"3eb16576-3cf7-44c0-a7e5-5050bd06e58b"],

Cell[BoxData[
 RowBox[{"TableForm", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"alphabet", ",", "substitution"}], "}"}], ",", 
   RowBox[{"TableSpacing", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"1", ",", " ", "1"}], "}"}]}]}], "]"}]], "Input",ExpressionUUID->\
"5b76a3f1-22b6-4b25-988b-35ca92a38f5c"],

Cell["The substitutions for encryption and decryption are:", "Text",ExpressionUUID->"8503a0cd-cb62-4ad3-90a7-4d96526751d9"],

Cell[BoxData[
 RowBox[{"encrypt", "=", 
  RowBox[{"Thread", "[", 
   RowBox[{"alphabet", "\[Rule]", "substitution"}], "]"}]}]], "Input",Expressi\
onUUID->"44408965-33c8-4fb6-8439-b85c8dd78d74"],

Cell[BoxData[
 RowBox[{"decrypt", "=", 
  RowBox[{"Thread", "[", 
